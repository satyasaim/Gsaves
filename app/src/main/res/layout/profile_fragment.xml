<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
   android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:descendantFocusability="beforeDescendants"
    android:paddingTop="@dimen/nav_drawer_top_padding">

<FrameLayout
    android:id="@+id/lay_proffrag1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:descendantFocusability="beforeDescendants">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:descendantFocusability="beforeDescendants"
        android:focusable="true"
        android:focusableInTouchMode="true">

        <TextView
            android:id="@+id/tv_card_add_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="38dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"

            android:fontFamily="@font/droidserif_bold"
            android:gravity="center"
            android:text="Profile"
            android:textAllCaps="true"
            android:textSize="18sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.496"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="80dp"
            android:layout_height="80dp"
            android:layout_marginStart="167dp"
            android:layout_marginLeft="167dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="167dp"
            android:layout_marginRight="167dp"
            android:maxWidth="80dp"
            android:maxHeight="80dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_card_add_title"
            app:srcCompat="@drawable/profile" />

        <TextView
            android:id="@+id/tv_profile_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="177dp"
            android:layout_marginLeft="177dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="177dp"
            android:layout_marginRight="177dp"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/droidserif_bold"
            android:gravity="center"
            android:text="Johnny"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView3" />

        <EditText
            android:id="@+id/et_prof_name"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"


            android:fontFamily="@font/droidserif_regular"
            android:hint="First Name"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="Supremo Auto"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_profile_name" />

        <EditText
            android:id="@+id/et_card_name"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"
            android:ems="10"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/droidserif_regular"
            android:hint="Last Name"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="Bill Belemy"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_prof_name" />

        <EditText
            android:id="@+id/et_card_id"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"
            android:ems="10"
            android:fontFamily="@font/droidserif_regular"
            android:hint="Email"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="Johnny.supreme@gmail.com"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.492"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_card_name" />

        <EditText
            android:id="@+id/et_card_cvv"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"
            android:ems="10"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/droidserif_regular"
            android:hint="Mobile"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="813-637-7738"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_card_id" />

        <EditText
            android:id="@+id/et_profile_address"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"
            android:ems="10"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/droidserif_regular"
            android:hint="Address"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="5667 Forest Lane. Tampa FL."
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_card_cvv" />

        <EditText
            android:id="@+id/et_profile_zip"
            android:layout_width="280dp"
            android:layout_height="30dp"
            android:layout_marginStart="99dp"
            android:layout_marginLeft="99dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="99dp"
            android:layout_marginRight="99dp"
            android:autoSizeTextType="uniform"
            android:background="@drawable/layout_bg"
            android:ems="10"
            android:focusableInTouchMode="true"
            android:fontFamily="@font/droidserif_regular"
            android:hint="Address"
            android:inputType="textPersonName"
            android:paddingLeft="10dp"
            android:text="33718"
            android:textSize="14sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.507"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_profile_address" />

        <TextView
            android:id="@+id/tv_change_pwd"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp"
            android:fontFamily="@font/droidserif_regular"
            android:text="@string/chg_pwd"
            android:textStyle="bold"
            app:layout_constraintBottom_toTopOf="@+id/bt_card_register"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/et_profile_zip" />

        <Button
            android:id="@+id/bt_card_register"
            android:layout_width="280dp"
            android:layout_height="37dp"
            android:layout_marginStart="40dp"
            android:layout_marginLeft="40dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="40dp"

            android:layout_marginRight="40dp"
            android:background="@drawable/signup"
            android:fontFamily="@font/droidserif_regular"
            android:text="Save"

            android:textColor="@color/fontColor"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tv_change_pwd"
            tools:textSize="20sp" />
    </android.support.constraint.ConstraintLayout>

</FrameLayout>

</LinearLayout>